{
	"name": "Voice In Bulk",
	"version": "1.1",
	"description": "Parses VK voice messages to text",
	"permissions": [
		"storage",
		"notifications"
	],

	"content_scripts": [
		{
			"matches": ["*://vk.com/im*"],
			"js": [
				"arrive.min.js",
				"content_script.js"
			],
			"css": ["styles.css"]
		}
	],

	"manifest_version": 2,

	// crbug.com/134315
	"web_accessible_resources": [
		"48.png"
	]
}
{
  "name": "VRP extension",
  "manifest_version": 2,
  "version": "0.1",
  "description": "Parses VK voice messages to text",
  "content_scripts": [
    {
      "matches": [
        "*://vk.com/im*"
      ],
      "js": [
        "arrive.js",
        "script.js"
      ],
      "css": [
        "styles.css"
      ]
    }
  ],
  "permissions": [
    "storage",
    "notifications"
  ]
}